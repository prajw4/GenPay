# Copy this file to .env and fill in the values for local development
# Example MongoDB connection string (replace <user>, <pass>, <host> and <dbname>):
MONGODB_URI=mongodb+srv://<user>:<pass>@cluster0.example.mongodb.net/<dbname>?retryWrites=true&w=majority

# JWT secret used to sign tokens
JWT_SECRET=your_jwt_secret_here

# Optional: port to run server on
PORT=3000
